package Module2_Practice;

class A{
	public void m1() {
		System.out.println("A-class m1 method");
	}
}
class B extends A{
	public void m2() {
		System.out.println("B-class m2 method");
	}
}
public class Upcast_Downcast {
	public static void main(String[] args) {
     A a1=new B();
     B b1=(B) a1;
     a1.m1();
     b1.m1();
     b1.m2();
	
	
	}

}
