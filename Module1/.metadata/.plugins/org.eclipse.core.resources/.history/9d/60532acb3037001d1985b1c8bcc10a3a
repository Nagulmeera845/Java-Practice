
public class Module1 {

	public static void main(String[] args) {
    int daytype=2;
    
	switch(daytype)
    {
    case 1:
    case 2:
    case 3:
    break;
    case 4:
    case 5:System.out.println("weekday");
    case 6:System.out.println("weekend-1");
    case 7:System.out.println("weekend-2");
    }
	}

}
