package Loops;
import java.util.Scanner;
public class ForLoop {
	public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    System.out.println("Enter number:");
    int n=sc.nextInt();
     int count=0;
     for(int i=1;i<=n;i++) {
    	 count=count+1;
     }
     System.out.println("Count of i to n is:"+count);
   	}

}
