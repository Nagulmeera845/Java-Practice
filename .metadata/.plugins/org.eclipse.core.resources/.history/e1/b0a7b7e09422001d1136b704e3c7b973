package Loops;
import java.util.Scanner;
public class ForLoop {
	public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    System.out.println("Enter number:");
    int num=sc.nextInt();
     int count=0;
     for(int i=1;i<=num;i++) {
    	 count++;
     }
     System.out.println("Count of i to n is:"+count);
	}

}
